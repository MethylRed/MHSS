<UserControl x:Class="MHSS.Views.SubViews.Solution"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:MHSS.Views.SubViews"
             xmlns:subview="clr-namespace:MHSS.ViewModels.SubView"
             mc:Ignorable="d" 
             d:DataContext="{d:DesignInstance Type=subview:SolutionViewModel}"
             d:DesignHeight="450" d:DesignWidth="800">
    <Expander Margin="5" Height="Auto" Width="Auto" VerticalAlignment="Stretch" HorizontalAlignment="Stretch"
              BorderThickness="2" Background="White" BorderBrush="black">
        <Expander.Header>
            <TextBlock>
                <Run>test</Run>
            </TextBlock>
        </Expander.Header>
        <TextBlock Text="{Binding disp.Value, Mode=OneWay}" FontSize="14" Foreground="Black"/>
    </Expander>
</UserControl>
